{
  "private": true,
  "scripts": {
    "lint": "eslint src tools",
    "relay-compiler": "relay-compiler --watchman false --src src --exclude 'data/**' --schema src/data/schema.graphql",
    "start": "npm run relay-compiler && babel-node src/server",
    "update-schema": "babel-node tools/updateSchema.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run update-schema",
      "git add src/data/schema.*"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@4c/babel-preset": "^7.3.6",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/runtime": "^7.10.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-relay": "^10.0.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^2.6.11",
    "eslint": "^7.4.0",
    "eslint-config-4catalyzer-react": "^1.0.8",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "farce": "^0.4.1",
    "found": "^0.5.3",
    "found-relay": "^0.8.0",
    "graphql": "^15.3.0",
    "graphql-relay": "^0.6.0",
    "husky": "^4.2.5",
    "isomorphic-fetch": "^2.2.1",
    "keycode": "^2.2.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^10.0.0",
    "react-relay-network-modern": "~4.2.0",
    "react-relay-network-modern-ssr": "~1.2.4",
    "regenerator-runtime": "^0.13.5",
    "relay-compiler": "^10.0.0",
    "relay-runtime": "^10.0.0",
    "serialize-javascript": "^4.0.0",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "^3.7.2"
  }
}
